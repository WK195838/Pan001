          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="dtPYR002TableAdapter" GeneratorDataComponentClassName="dtPYR002TableAdapter" Name="dtPYR002" UserDataComponentName="dtPYR002TableAdapter">
            <MainSource>
              <DbSource ConnectionRef="MyConnectionString (Web.config)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>Select M.*,S.* From
(Select M.[Company],M.DeptId,M.[DepName],M.[CompanyName],M.[SalaryYM],M.[PeriodCode]
,Sum([LI_Fee]) As [LI_Fee],Sum([HI_Fee]) As [HI_Fee]
,Sum(WTP) As WTP ,Sum(WTM) As WTM,Sum(WT) As WT ,Sum(NTP) As NTP,Sum(NTM) As NTM,Sum(NT)  As NT,Sum(TAX) As TAX,Sum(PAY+BaseSalaryAct) As PAYAct
From
(
SELECT PWH.[Company],ISNULL(D.DepCode, PM.DeptId) AS DeptId,IsNull([DepName],[DeptId]) As [DepName],C.[CompanyName]  
,PWH.[EmployeeId],PM.[EmployeeName],PWH.[SalaryYM],PWH.[PeriodCode],[BaseSalary],[Paydays]
,[LeaveHours_deduction]
,[TaxRate]  ,[NT_P],[WT_P_Salary],[NT_M],[LI_Fee],[HI_Fee],[HI_Person],[WT_P_Bonus],[WT_M_Salary]
,[WT_M_Bonus],[P1_borrowing],[WT_Overtime]  ,[NT_Overtime],[OnWatch],[WT_Overtime_Fee],[NT_Overtime_Fee]
,[OnWatch_Fee]  ,PWD.WTP,PWD.WTM,PWD.WT ,PWD.NTP,PWD.NTM,PWD.NT ,PWD.TAX,(PWD.WT+PWD.NT-PWD.TAX) As PAY
,(Select IsNull(PWD.SalaryAmount,0) from PayrollWorking_Detail PWD Where PWD.Company = PWH.Company And PWD.EmployeeId = PWH.EmployeeId And PWD.SalaryYM = PWH.SalaryYM And PWD.PeriodCode = PWH.PeriodCode And PWD.SalaryItem='01') As BaseSalaryAct  
FROM [PayrollWorking_Heading] PWH Left Join [Personnel_Master] PM On PWH.[Company] = PM.[Company] And PWH.[EmployeeId] = PM.[EmployeeId] Left Join [Company] C On PWH.[Company] = C.[Company]  Left Join [Department] D On PM.[Company] = D.[Company] And  PM.[DeptId] = D.[DepCode]  Left Join (  SELECT PWD.[Company],PWD.[EmployeeId],PWD.[SalaryYM],PWD.[PeriodCode]  ,Sum(Case SalaryId When '03' then 0 else (Case When NWTax='Y' then (Case When Convert(int,SalaryId) &lt;= 1 then 0 else (Case PMType When 'A' then (IsNull(PWD.SalaryAmount,0)+IsNull(PMO.SalaryAmount,0)) else (0-(IsNull(PWD.SalaryAmount,0)+IsNull(PMO.SalaryAmount,0))) end) end) else 0 end) end) WT  ,Sum(Case SalaryId When '03' then 0 else (Case When NWTax='Y' then (Case When Convert(int,SalaryId) &lt;= 1 then 0 else (Case PMType When 'A' then (IsNull(PWD.SalaryAmount,0)+IsNull(PMO.SalaryAmount,0)) else 0 end) end) else 0 end) end) WTP  ,Sum(Case SalaryId When '03' then 0 else (Case When NWTax='Y' then (Case When Convert(int,SalaryId) &lt;= 1 then 0 else (Case PMType When 'B' then (IsNull(PWD.SalaryAmount,0)+IsNull(PMO.SalaryAmount,0)) else 0 end) end) else 0 end) end) WTM  ,Sum(Case SalaryId When '03' then 0 else (Case When NWTax='N' then (Case When Convert(int,SalaryId) &lt;= 1 then 0 else (Case PMType When 'A' then (IsNull(PWD.SalaryAmount,0)+IsNull(PMO.SalaryAmount,0)) else (0-(IsNull(PWD.SalaryAmount,0)+IsNull(PMO.SalaryAmount,0))) end) end) else 0 end) end) NT  ,Sum(Case SalaryId When '03' then 0 else (Case When NWTax='N' then (Case When Convert(int,SalaryId) &lt;= 1 then 0 else (Case PMType When 'A' then (IsNull(PWD.SalaryAmount,0)+IsNull(PMO.SalaryAmount,0)) else 0 end) end) else 0 end) end) NTP  ,Sum(Case SalaryId When '03' then 0 else (Case When NWTax='N' then (Case When Convert(int,SalaryId) &lt;= 1 then 0 else (Case PMType When 'B' then (IsNull(PWD.SalaryAmount,0)+IsNull(PMO.SalaryAmount,0)) else 0 end) end) else 0 end) end) NTM  ,Sum(Case SalaryId When '03' then IsNull(PWD.SalaryAmount,0) else 0 end) TAX   FROM PayrollWorking_Detail PWD   Left join Payroll_Master_OtherDetail PMO On PWD.Company = PMO.Company And PWD.EmployeeId = PMO.EmployeeId And PWD.SalaryYM = PMO.SalaryYM And PWD.PeriodCode = PMO.PeriodCode And PWD.SalaryItem = PMO.SalaryItem    Left join SalaryStructure_Parameter SP On PWD.SalaryItem = SP.SalaryId   Group By PWD.[Company],PWD.[EmployeeId],PWD.[SalaryYM],PWD.[PeriodCode]  ) PWD On PWH.[Company] = PWD.[Company] And PWH.[EmployeeId] = PWD.[EmployeeId] And PWD.[SalaryYM] = PWH.[SalaryYM] And PWD.[PeriodCode] = PWH.[PeriodCode] Where PWH.Company='01' And PWH.SalaryYM = 201009 And PWH.PeriodCode = '0'
) M
