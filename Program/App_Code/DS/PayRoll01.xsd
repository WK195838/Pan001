          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="dtPYR001_SubTableAdapter" GeneratorDataComponentClassName="dtPYR001_SubTableAdapter" Name="dtPYR001_Sub" UserDataComponentName="dtPYR001_SubTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="MyConnectionString (Web.config)" DbObjectName="SELECT PWD.Company, PWD.EmployeeId, PWD.SalaryYM, PWD.PeriodCode, PWD.SalaryItem, PWD.SalaryAmount, ISNULL(PMO.SalaryAmount, 0) AS OtherAmount, ISNULL(PWD.SalaryAmount, 0) + ISNULL(PMO.SalaryAmount, 0) AS TotalAmount, SP.SalaryName, (CASE PMType WHEN 'A' THEN '加項' ELSE '減項' END) AS PMType, SP.NWTax, SP.RegularPay, (CASE WHEN CONVERT(int, SalaryId) &lt;= 1 THEN 0 ELSE (CASE PMType WHEN 'A' THEN (IsNull(PWD.SalaryAmount, 0) + IsNull(PMO.SalaryAmount, 0)) ELSE (0 - (IsNull(PWD.SalaryAmount, 0) + IsNull(PMO.SalaryAmount, 0))) END) END) AS OtherNWSalary, SP.SalaryMasterList AS SML FROM PayrollWorking_Detail AS PWD LEFT OUTER JOIN Payroll_Master_OtherDetail AS PMO ON PWD.Company = PMO.Company AND PWD.EmployeeId = PMO.EmployeeId AND PWD.SalaryYM = PMO.SalaryYM AND PWD.PeriodCode = PMO.PeriodCode AND PWD.SalaryItem = PMO.SalaryItem LEFT OUTER JOIN SalaryStructure_Parameter AS SP ON PWD.SalaryItem = SP.SalaryId WHERE (PWD.SalaryYM = 201009) AND (PWD.EmployeeId IN ('A1619'))" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT         Company, EmployeeId, SalaryYM, PeriodCode, 
                          SUM(CASE SML WHEN 1 THEN OtherNWSalary ELSE 0 END) AS F01, 
                          SUM(CASE SML WHEN 2 THEN OtherNWSalary ELSE 0 END) AS F02, 
                          SUM(CASE SML WHEN 3 THEN OtherNWSalary ELSE 0 END) AS F03, 
                          SUM(CASE SML WHEN 4 THEN OtherNWSalary ELSE 0 END) AS F04, 
                          SUM(CASE SML WHEN 5 THEN OtherNWSalary ELSE 0 END) AS F05, 
                          SUM(CASE SML WHEN 6 THEN OtherNWSalary ELSE 0 END) AS F06, 
                          SUM(CASE SML WHEN 7 THEN OtherNWSalary ELSE 0 END) AS F07, 
                          SUM(CASE SML WHEN 8 THEN OtherNWSalary ELSE 0 END) AS F08, 
                          SUM(CASE SML WHEN 9 THEN OtherNWSalary ELSE 0 END) AS F09, 
                          SUM(CASE SML WHEN 10 THEN OtherNWSalary ELSE 0 END) AS F10, 
                          SUM(CASE SML WHEN 11 THEN OtherNWSalary ELSE 0 END) AS F11, 
                          SUM(CASE SML WHEN 12 THEN OtherNWSalary ELSE 0 END) AS F12, 
                          SUM(CASE SML WHEN 13 THEN OtherNWSalary ELSE 0 END) AS F13, 
                          SUM(CASE SML WHEN 14 THEN OtherNWSalary ELSE 0 END) AS F14, 
                          SUM(CASE SML WHEN 15 THEN OtherNWSalary ELSE 0 END) AS F15, 
                          SUM(CASE SML WHEN 16 THEN OtherNWSalary ELSE 0 END) AS F16, 
                          SUM(CASE SML WHEN 17 THEN OtherNWSalary ELSE 0 END) AS F17, 
                          SUM(CASE SML WHEN 18 THEN OtherNWSalary ELSE 0 END) AS F18, 
                          SUM(CASE SML WHEN 19 THEN OtherNWSalary ELSE 0 END) AS F19, 
                          SUM(CASE SML WHEN 20 THEN OtherNWSalary ELSE 0 END) AS F20
FROM             (SELECT         PWD.Company, PWD.EmployeeId, PWD.SalaryYM, 
                                                      PWD.PeriodCode, PWD.SalaryItem, PWD.SalaryAmount, 
                                                      ISNULL(PMO.SalaryAmount, 0) AS OtherAmount, 
                                                      ISNULL(PWD.SalaryAmount, 0) + ISNULL(PMO.SalaryAmount, 0) 
                                                      AS TotalAmount, SP.SalaryName, 
                                                      (CASE PMType WHEN 'A' THEN '加項' ELSE '減項' END) 
                                                      AS PMType, SP.NWTax, SP.RegularPay, 
                                                      (CASE WHEN CONVERT(int, SalaryId) 
                                                      &lt;= 1 THEN 0 ELSE (CASE PMType WHEN 'A' THEN (IsNull(PWD.SalaryAmount,
                                                       0) + IsNull(PMO.SalaryAmount, 0)) 
                                                      ELSE (0 - (IsNull(PWD.SalaryAmount, 0) 
                                                      + IsNull(PMO.SalaryAmount, 0))) END) END) AS OtherNWSalary, 
                                                      SP.SalaryMasterList AS SML
                           FROM              PayrollWorking_Detail AS PWD LEFT OUTER JOIN
                                                      Payroll_Master_OtherDetail AS PMO ON 
                                                      PWD.Company = PMO.Company AND 
                                                      PWD.EmployeeId = PMO.EmployeeId AND 
                                                      PWD.SalaryYM = PMO.SalaryYM AND 
                                                      PWD.PeriodCode = PMO.PeriodCode AND 
                                                      PWD.SalaryItem = PMO.SalaryItem LEFT OUTER JOIN
                                                      SalaryStructure_Parameter AS SP ON 
                                                      PWD.SalaryItem = SP.SalaryId
                           WHERE          (PWD.SalaryYM = 201009) AND (PWD.EmployeeId IN ('A1619'))) 
                          AS Sub
GROUP BY  Company, EmployeeId, SalaryYM, PeriodCode</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
