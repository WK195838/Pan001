                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT         PWH.Company, ISNULL(D.DepCode, PM.DeptId) AS DeptId, 
                          ISNULL(D.DepName, PM.DeptId) AS DepName, C.CompanyName, 
                          PWH.EmployeeId, PM.EmployeeName, PWH.SalaryYM, PWH.PeriodCode, 
                          PWH.BaseSalary, PWH.Paydays, PWH.LeaveHours_deduction, PWH.TaxRate, 
                          PWH.NT_P, PWH.WT_P_Salary, PWH.NT_M, PWH.LI_Fee, PWH.HI_Fee, 
                          PWH.HI_Person, PWH.WT_P_Bonus, PWH.WT_M_Salary, PWH.WT_M_Bonus, 
                          PWH.P1_borrowing, PWH.WT_Overtime, PWH.NT_Overtime, PWH.OnWatch, 
                          PWH.WT_Overtime_Fee, PWH.NT_Overtime_Fee, PWH.OnWatch_Fee, 
                          PWD_1.WTP, PWD_1.WTM, PWD_1.WT, PWD_1.NTP, PWD_1.NTM, 
                          PWD_1.NT, PWD_1.TAX, 
                          PWH.BaseSalary + PWD_1.WT + PWD_1.NT - PWD_1.TAX AS PAY, 
                          0 AS BaseSalaryAct
FROM             PayrollWorking_Heading AS PWH LEFT OUTER JOIN
                          Personnel_Master AS PM ON PWH.Company = PM.Company AND 
                          PWH.EmployeeId = PM.EmployeeId LEFT OUTER JOIN
                          Company AS C ON PWH.Company = C.Company LEFT OUTER JOIN
                          Department AS D ON PM.Company = D.Company AND 
                          PM.DeptId = D.DepCode LEFT OUTER JOIN
                              (SELECT         PWD.Company, PWD.EmployeeId, PWD.SalaryYM, 
                                                           PWD.PeriodCode, SUM(CASE CONVERT(int, SalaryId) 
                                                           WHEN 5 THEN 0 ELSE (CASE WHEN NWTax = 'Y' THEN (CASE WHEN
                                                            CONVERT(int, SalaryId) 
                                                           &lt;= 1 THEN 0 ELSE (CASE PMType WHEN 'A' THEN (IsNull(PWD.SalaryAmount,
                                                            0) + IsNull(PMO.SalaryAmount, 0)) 
                                                           ELSE (0 - (IsNull(PWD.SalaryAmount, 0) 
                                                           + IsNull(PMO.SalaryAmount, 0))) END) END) ELSE 0 END) END) 
                                                           AS WT, SUM(CASE CONVERT(int, SalaryId) 
                                                           WHEN 5 THEN 0 ELSE (CASE WHEN NWTax = 'Y' THEN (CASE WHEN
                                                            CONVERT(int, SalaryId) 
                                                           &lt;= 1 THEN 0 ELSE (CASE PMType WHEN 'A' THEN (IsNull(PWD.SalaryAmount,
                                                            0) + IsNull(PMO.SalaryAmount, 0)) ELSE 0 END) END) 
                                                           ELSE 0 END) END) AS WTP, SUM(CASE CONVERT(int, SalaryId) 
                                                           WHEN 5 THEN 0 ELSE (CASE WHEN NWTax = 'Y' THEN (CASE WHEN
                                                            CONVERT(int, SalaryId) 
                                                           &lt;= 1 THEN 0 ELSE (CASE PMType WHEN 'B' THEN (IsNull(PWD.SalaryAmount,
                                                            0) + IsNull(PMO.SalaryAmount, 0)) ELSE 0 END) END) 
                                                           ELSE 0 END) END) AS WTM, SUM(CASE CONVERT(int, SalaryId) 
                                                           WHEN 5 THEN 0 ELSE (CASE WHEN NWTax = 'N' THEN (CASE WHEN
                                                            CONVERT(int, SalaryId) 
                                                           &lt;= 1 THEN 0 ELSE (CASE PMType WHEN 'A' THEN (IsNull(PWD.SalaryAmount,
                                                            0) + IsNull(PMO.SalaryAmount, 0)) 
                                                           ELSE (0 - (IsNull(PWD.SalaryAmount, 0) 
                                                           + IsNull(PMO.SalaryAmount, 0))) END) END) ELSE 0 END) END) 
                                                           AS NT, SUM(CASE CONVERT(int, SalaryId) 
                                                           WHEN 5 THEN 0 ELSE (CASE WHEN NWTax = 'N' THEN (CASE WHEN
                                                            CONVERT(int, SalaryId) 
                                                           &lt;= 1 THEN 0 ELSE (CASE PMType WHEN 'A' THEN (IsNull(PWD.SalaryAmount,
                                                            0) + IsNull(PMO.SalaryAmount, 0)) ELSE 0 END) END) 
                                                           ELSE 0 END) END) AS NTP, SUM(CASE CONVERT(int, SalaryId) 
                                                           WHEN 5 THEN 0 ELSE (CASE WHEN NWTax = 'N' THEN (CASE WHEN
                                                            CONVERT(int, SalaryId) 
                                                           &lt;= 1 THEN 0 ELSE (CASE PMType WHEN 'B' THEN (IsNull(PWD.SalaryAmount,
                                                            0) + IsNull(PMO.SalaryAmount, 0)) ELSE 0 END) END) 
                                                           ELSE 0 END) END) AS NTM, SUM(CASE CONVERT(int, SalaryId) 
                                                           WHEN 5 THEN IsNull(PWD.SalaryAmount, 0) ELSE 0 END) 
                                                           AS TAX
                                FROM              PayrollWorking_Detail AS PWD LEFT OUTER JOIN
                                                           Payroll_Master_OtherDetail AS PMO ON 
                                                           PWD.Company = PMO.Company AND 
                                                           PWD.EmployeeId = PMO.EmployeeId AND 
                                                           PWD.SalaryYM = PMO.SalaryYM AND 
                                                           PWD.PeriodCode = PMO.PeriodCode AND 
                                                           PWD.SalaryItem = PMO.SalaryItem LEFT OUTER JOIN
                                                           SalaryStructure_Parameter AS SP ON 
                                                           PWD.SalaryItem = SP.SalaryId
                                GROUP BY   PWD.Company, PWD.EmployeeId, PWD.SalaryYM, 
                                                           PWD.PeriodCode) AS PWD_1 ON 
                          PWH.Company = PWD_1.Company AND 
                          PWH.EmployeeId = PWD_1.EmployeeId AND 
                          PWD_1.SalaryYM = PWH.SalaryYM AND 
                          PWD_1.PeriodCode = PWH.PeriodCode</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
